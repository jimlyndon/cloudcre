@model Cloudcre.Service.ViewModels.IndustrialViewModel

@{
    ViewBag.Title = "Cloud CRE - Create new industrial property";
    Layout = "~/Views/Shared/Property/_Layout_Property_Create.cshtml";
}

@section scripts {
    <script type="text/javascript">
        window.cloudcre.routing.urls({
            action: '@Url.Action("Create", "Industrial")',
            model: '@Url.Action("Model", "Industrial")'
        });
    </script>
    <script type="text/html" id="template">
        @using (Html.BeginForm("Create", "Industrial", FormMethod.Post, new { id = "dialog-form", @class = "wufoo" }))
        {
            @Html.Partial("_Template")
        }
    </script>
}